        <shape>
            <solid android:color="@color/interactive_pressed" />
            <corners android:radius="8dp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/interactive_focused" />
            <corners android:radius="8dp" />
        </shape>
    </item>
